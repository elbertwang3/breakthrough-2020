    {
      "date_text": "31 December 2019",
      "date": "12/29/2019",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Health officials in Wuhan, China, report a cluster of mysterious pneumonia cases."
    },
    {
      "date_text": "8 January",
      "date": "1/8/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "A novel coronavirus is publicly identified as the <a href= \"https://www.sciencemag.org/news/2020/01/scientists-urge-china-quickly-share-data-virus-linked-pneumonia-outbreak\">source of the outbreak</a>."
    },
    {
      "date_text": "10 January",
      "date": "1/10/2020",
      "type": "FINDINGS",
      "text": "Genetic sequence posted to <a href= \"http://virological.org/>virological.org</a>"
    },
    {
      "date_text": "20 January",
      "date": "1/20/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Human-to-human transmission <a href=\"https://www.sciencemag.org/news/2020/01/arrival-sars-new-virus-united-states-heightens-concerns-about-global-spread%20(this%20link%20is%20for%20human-to-human%20transmission)\">confirmed </a>",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "23 January",
      "date": "1/23/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Wuhan lockdown begins"
    },
    {
      "date_text": "23 January",
      "date": "1/23/2020",
      "type": "FINDINGS",
      "text": "The novel coronavirus is <a href=\"https://www.nature.com/articles/s41586-020-2012-7\">96.2%</a> similar to a virus in bats.",
      "img": "bat_timeline.png"
    },
    {
      "date_text": "30 January",
      "date": "1/30/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "WHO declares the outbreak a Public Health Emergency of International Concern.",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "5 February",
      "date": "2/5/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "<i>Diamond Princess</i> cruise ship begins its 2-week quarantine."
    },
    {
      "date_text": "11 February",
      "date": "2/11/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "The virus <a href=\"https://www.sciencemag.org/news/2020/02/bit-chaotic-christening-new-coronavirus-and-its-disease-name-create-confusion\">gets its name</a>."
    },
    {
      "date_text": "Mid-February",
      "date": "2/13/2020",
      "type": "FINDINGS",
      "text": "Age, hypertension, diabetes, heart disease, and male sex are all confirmed as <a href=\"https://www.thelancet.com/journals/lancet/article/PIIS0140-6736(20)30566-3/fulltext\">risk factors for severe disease</a>.",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "17 February",
      "date": "2/17/2020",
      "type": "FINDINGS",
      "text": "Widespread <a href=\"https://science.sciencemag.org/content/368/6490/489\">undocumented infection </a> led to the rapid spread of COVID-19."
    },
    {
      "date_text": "19 February",
      "date": "2/23/2020",
      "type": "FINDINGS",
      "text": "Scientists image the <a href=\"https://science.sciencemag.org/content/367/6483/1260\">atomic-level structure</a> of the spike protein.\n",
      "img": "virus_timeline.png"
    },
    {
      "date_text": "23 February",
      "date": "2/23/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "First major European outbreak in Italy"
    },
    {
      "date_text": "11 March",
      "date": "3/11/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "WHO declares the coronavirus outbreak a pandemic.\r\n",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "16 March",
      "date": "3/16/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Moderna and CanSino <a href=\"https://www.sciencemag.org/news/2020/03/record-setting-speed-vaccine-makers-take-their-first-shots-new-coronavirus\"vaccine trials launch</a>."
    },
    {
      "date_text": "16 March",
      "date": "3/16/2020",
      "type": "FINDINGS",
      "text": "Imperial College London model suggests <a href=\"https://www.imperial.ac.uk/media/imperial-college/medicine/sph/ide/gida-fellowships/Imperial-College-COVID19-NPI-modelling-16-03-2020.pdf\"health care systems will be overwhelmed</a>."
    },
    {
      "date_text": "26 March",
      "date": "3/26/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "New York City becomes the epicenter of the U.S. outbreak.\r\n",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "26 March",
      "date": "3/26/2020",
      "type": "FINDINGS",
      "text": "Widespread viral RNA in hospital rooms suggests <a href=\"https://www.nature.com/articles/s41598-020-69286-3\">airborne transmission</a>.",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "28 March",
      "date": "3/28/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "U.S. regulators issue emergency use authorization (EUA) for hydroxychloroquine."
    },
    {
      "date_text": "Early April",
      "date": "4/1/2020",
      "type": "FINDINGS",
      "text": "COVID-19 can damage <a href=\"https://www.sciencemag.org/news/2020/04/how-does-coronavirus-kill-clinicians-trace-ferocious-rampage-through-body-brain-toes\">multiple organ systems</a>, including the heart, blood vessels, kidneys, and brain.",
      "img": "brain_timeline.png"
    },
    {
      "date_text": "3 April",
      "date": "4/3/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "U.S. public health officials suggest people wear cloth masks.",
      "img": "mask_timeline.png"
    },
    {
      "date_text": "19 April",
      "date": "4/18/2020",
      "type": "FINDINGS",
      "text": "First evidence that a COVID-19 vaccine <a href=\"https://www.sciencemag.org/news/2020/04/covid-19-vaccine-protects-monkeys-new-coronavirus-chinese-biotech-reports\">can protect monkeys</a>",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "24 April",
      "date": "4/24/2020",
      "type": "FINDINGS",
      "text": "<a href=\"https://www.nejm.org/doi/full/10.1056/NEJMoa2008457\">Asymptomatic carriers</a> play a large role in COVID-19 transmission.",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "27 April",
      "date": "4/27/2020",
      "type": "FINDINGS",
      "text": "Remdesivir <a href=\"https://www.nejm.org/doi/10.1056/NEJMoa2007764\">reduces hospital stays</a> in severely ill patients."
    },
    {
      "date_text": "1 May",
      "date": "5/1/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "U.S. regulators issue EUA for remdesivir."
    },
    {
      "date_text": "15 May",
      "date": "5/15/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Operation Warp Speed vaccine project <a href=\"https://www.sciencemag.org/news/2020/05/unveiling-warp-speed-white-house-s-america-first-push-coronavirus-vaccine\">launches</a>."
    },
    {
      "date_text": "4 June",
      "date": "6/4/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Major medical journals <a href=\"https://www.sciencemag.org/news/2020/06/two-elite-medical-journals-retract-coronavirus-papers-over-data-integrity-questions\">retract coronavirus papers</a> over fabricated data.",
      "img": "paper_timeline.png"
    },
    {
      "date_text": "5 June",
      "date": "6/6/2020",
      "type": "FINDINGS",
      "text": "Hydroxychloroquine <a href=\"https://www.nejm.org/doi/full/10.1056/NEJMoa2022926\">does not reduce death</a> in hospitalized patients."
    },
    {
      "date_text": "15 June",
      "date": "6/15/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "U.S. regulators revoke EUA for hydroxychloroquine."
    },
    {
      "date_text": "16 June",
      "date": "6/17/2020",
      "type": "FINDINGS",
      "text": "Dexamethasone reduces fatality by <a href=\"https://www.sciencemag.org/news/2020/06/cheap-steroid-first-drug-shown-reduce-death-covid-19-patients\">up to one-third</a> in hospitalized patients."
    },
    {
      "date_text": "6 July",
      "date": "7/6/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "United States sends a formal notice of withdrawal from WHO."
    },
    {
      "date_text": "9 July",
      "date": "7/9/2020",
      "type": "FINDINGS",
      "text": "Early studies hint at <a href=\"https://jamanetwork.com/journals/jama/fullarticle/2768351\">the lingering symptoms</a> of “long COVID.”"
    },
    {
      "date_text": "25 August",
      "date": "8/25/2020",
      "type": "FINDINGS",
      "text": "First <a href=\"https://academic.oup.com/cid/advance-article/doi/10.1093/cid/ciaa1275/5897019\">confirmed case</a> of reinfection"
    },
    {
      "date_text": "9 September",
      "date": "9/9/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Two-thirds of countries have joined COVAX, an international vaccine distribution effort."
    },
    {
      "date_text": "24 September",
      "date": "9/24/2020",
      "type": "FINDINGS",
      "text": "Up to <a href=\"https://www.sciencemag.org/news/2020/09/hidden-immune-weakness-found-14-gravely-ill-covid-19-patients\">14% of severe cases</a> are linked to genetic factors.",
      "img": "DNA_timeline.png"
    },
    {
      "date_text": "Late October",
      "date": "10/27/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Europe and the United States confront their second and third waves."
    },
    {
      "date_text": "9 November",
      "date": "11/8/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Pfizer and BioNTech announce vaccine efficacy above 90%.",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "16 November",
      "date": "11/17/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Moderna reveals vaccine efficacy nearing 95%.",
      "img": "syringe_timeline.png",
      "yalign": "bottom"
    },
    {
      "date_text": "23 November",
      "date": "11/22/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "AstraZeneca reports vaccine efficacy <a href=\"https://www.sciencemag.org/news/2020/11/another-covid-19-vaccine-success-candidate-may-prevent-further-coronavirus-transmission\"> between 62% and 90%</a>.",
      "img": "",
      "yalign": "bottom"
    },
    {
      "date_text": "24 November",
      "date": "11/25/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "Russia's Gamaleya institute reports vaccine efficacy of <a href=\"https://www.sciencemag.org/news/2020/11/more-data-its-covid-19-vaccine-russia-institute-offers-new-evidence-success\">91.4%</a>."
    },
    {
      "date_text": "2 December",
      "date": "12/2/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "U.K. regulators are the <a href=\"https://www.sciencemag.org/news/2020/12/wonderful-news-wake-uk-greenlights-pfizers-covid-19-vaccine\"> first to authorize </a> the Pfizer-BioNTech COVID-19 vaccine."
    },
    {
      "date_text": "9 December",
      "date": "12/9/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "China National Biotec Group vaccine has <a href=\"https://www.sciencemag.org/news/2020/12/wonderful-news-wake-uk-greenlights-pfizers-covid-19-vaccine\"> 86% efficacy</a>, according to UAE health ministry."
    },
    {
      "date_text": "14 December",
      "date": "12/17/2020",
      "type": "POLICY AND PUBLIC HEALTH",
      "text": "U.S. health care workers begin to receive Pfizer vaccine."
